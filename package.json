    "autoprefixer": "^8.4.1",
    "babel-loader": "^8.0.6",
    "babel-preset-airbnb": "^4.0.0",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "cssnano": "^4.1.10",
    "eslint": "^6.0.1",
