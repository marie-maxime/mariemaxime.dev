{
  "name": "mariemaxime-portfolio",
  "version": "0.0.1",
  "description": "Portfolio for Marie-Maxime",
  "main": "index.js",
  "author": "Marie-Maxime Tanguay",
  "license": "ISC",
  "dependencies": {
    "@stimulus/polyfills": "^1.1.1",
    "stimulus": "^1.1.1"
  },
  "scripts": {
    "server": "nodemon ./server.js",
    "dev": "cross-env NODE_ENV=development webpack --watch --config src/build/webpack.js",
    "build": "cross-env NODE_ENV=production webpack --config src/build/webpack.js",
    "render": "node render.js",
    "deploy": "yarn run lint && yarn run build && yarn run render",
    "lint": "yarn run lint:scripts && yarn run lint:styles",
    "lint-fix": "yarn run lint-fix:scripts && yarn run lint-fix:styles",
    "lint:scripts": "eslint --ext .js,.jsx,.ts,.tsx ./src/scripts/**/*.ts",
    "lint:styles": "stylelint ./src/styles/**/*.scss",
    "lint-fix:scripts": "eslint --fix ./src/scripts/**/*.ts",
    "lint-fix:styles": "stylelint --fix ./src/styles/**/*.scss"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
